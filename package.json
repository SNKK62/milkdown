{
  "name": "@milkdown/components",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SNKK62/milkdown-link-tooltip-plugin-nota.git"
  },
  "keywords": [
    "milkdown",
    "milkdown plugin"
  ],
  "engines": {
    "node": ">=17"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./link-tooltip": {
      "import": "./src/link-tooltip/index.ts"
    }
  },
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.es.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.es.js"
      },
      "./link-tooltip": {
        "types": "./lib/link-tooltip/index.d.ts",
        "import": "./lib/link-tooltip/index.es.js"
      }
    },
    "typesVersions": {
      "*": {
        "link-tooltip": [
          "./lib/link-tooltip/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "rollup -c -w",
    "test": "vitest",
    "tsc": "tsc --noEmit && echo",
    "build": "rimraf './lib' && tsc --emitDeclarationOnly && rollup -c && echo"
  },
  "peerDependencies": {
    "@codemirror/language": "^6",
    "@codemirror/state": "^6",
    "@codemirror/view": "^6",
    "@milkdown/core": "^7.2.0",
    "@milkdown/ctx": "^7.2.0",
    "@milkdown/plugin-tooltip": "^7.2.0",
    "@milkdown/preset-commonmark": "^7.2.0",
    "@milkdown/preset-gfm": "^7.2.0",
    "@milkdown/prose": "^7.2.0",
    "@milkdown/transformer": "^7.2.0",
    "@milkdown/utils": "^7.2.0"
  },
  "dependencies": {
    "@atomico/hooks": "^4.1.2",
    "@floating-ui/dom": "^1.5.1",
    "@milkdown/exception": "^7.5.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.throttle": "^4.1.9",
    "atomico": "^1.75.1",
    "clsx": "^2.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "tslib": "^2.5.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.0.12"
  },
  "devDependencies": {
    "@codemirror/language": "^6.10.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.0",
    "@milkdown/core": "^7.2.0",
    "@milkdown/ctx": "^7.2.0",
    "@milkdown/plugin-tooltip": "^7.2.0",
    "@milkdown/preset-commonmark": "^7.2.0",
    "@milkdown/preset-gfm": "^7.2.0",
    "@milkdown/prose": "^7.2.0",
    "@milkdown/transformer": "^7.2.0",
    "@milkdown/utils": "^7.2.0"
  }
}
